    var fileName = ""; // Variable globale pour stocker le nom de l'image
    var sectionAdded = false;

    function addSection() {

        if (!sectionAdded) {
            const imageSection = document.getElementById('imageSection');

            // Créer un nouvel élément de rubrique
            const newSection = document.createElement('section');

            // Créer un élément <input> pour sélectionner une image
            const inputFile = document.createElement('input');
            inputFile.type = 'file';
            inputFile.accept = 'image/*';
            inputFile.name = 'imageFile';
            inputFile.addEventListener('change', previewImage);


            // Créer un élément <img> pour afficher l'image
            const preview = document.createElement('img');
            /*preview.alt = 'Image Preview';*/
            preview.className = 'preview-image';
            // Créer un élément <button> pour activer le recadrage
            const cropButton = document.createElement('button');
            cropButton.textContent = 'Recadrer';
            cropButton.addEventListener('click', cropImage);
            cropButton.disabled = true;

            // Ajouter les éléments au nouvel élément de rubrique
            newSection.appendChild(inputFile);
            newSection.appendChild(preview);
            newSection.appendChild(cropButton);

            // Ajouter le nouvel élément de rubrique à la section existante
            imageSection.appendChild(newSection);

            sectionAdded = true; // Mettre à jour le drapeau pour indiquer que la section a été ajoutée
        }


    }

    function previewImage(event) {
        const input = event.target;
        const preview = input.nextSibling;

        // Vérifier si un fichier est sélectionné
        if (input.files && input.files[0]) {
            const reader = new FileReader();
            var file = input.files[0];

            // Lorsque le fichier est chargé
            reader.onload = function (e) {
                // Afficher l'image dans l'élément <img>
                preview.src = e.target.result;
            }

            // Lire le contenu du fichier en tant que données binaires
           reader.readAsDataURL(input.files[0]);

            fileName = file.name;
        }
    }

    function previewAndCropImage(event) {
        const input = event.target;
        const preview = input.nextSibling.nextSibling;
        const cropButton = input.nextSibling.nextSibling.nextSibling;

        // Vérifier si un fichier est sélectionné
        if (input.files && input.files[0]) {
            const reader = new FileReader();

            // Lorsque le fichier est chargé
            reader.onload = function (e) {
                // Afficher l'image dans l'élément <img>
                preview.src = e.target.result;

                // Activer le recadrage de l'image
                cropButton.disabled = false;
            }

            // Lire le contenu du fichier en tant que données binaires
            reader.readAsDataURL(input.files[0]);
        }
    }

    function cropImage() {
        const preview = this.previousSibling;

        // Créer une instance de Cropper.js avec l'élément <img> de prévisualisation
        const cropper = new Cropper(preview, {
